        "libs/api/repetition/shell/src/index.ts"
      ],
      "@voclearn/api/auth": ["libs/api/auth/src/index.ts"],
      "@voclearn/api/quiz": ["libs/api/quiz/src/index.ts"],
      "@voclearn/api/shared/application": [
        "libs/api/shared/application/src/index.ts"
      ],
      "@voclearn/api/shared/domain": ["libs/api/shared/domain/src/index.ts"],
      "@voclearn/api/shared/infrastructure/config": [
        "libs/api/shared/infrastructure/config/src/index.ts"
